package ch10_extends_interface.starcraft;

public class Zealot extends StarUnit{
	private int shieId;
	
	public Zealot() {
		super();
	}

	public Zealot(String name, int damage, int hp, int armor, int shieId) {
		//자식 생성자 맨위에 부모 생성자가 와야함
		super(name, damage, hp, armor);
		this.shield = shiedId;
	}
	
}
