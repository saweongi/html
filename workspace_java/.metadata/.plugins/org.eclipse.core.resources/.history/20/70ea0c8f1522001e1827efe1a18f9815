package practice.submult01;

public class result03 {

	public static void main(String[] args) {
		int example = 278;
		int i= example /100;
		int i1 = (example /10) %10;
		int i2 = example%10;
		int sum = i +i1 + i2;
		System.out.println("결과는 :" +sum);
		
		String x = example + "";
		
		int w = (int)x.charAt(0);
		System.out.println(w);
		

	}

}
