package practice.submult01;

public class result03 {

	public static void main(String[] args) {
		int example = 278;
		int i= example /100;
		int i1 = (example /10) %10;
		int i2 = example%10;
		int sum = i +i1 + i2;
		System.out.println("결과는 :" +sum);
		
		String x = example + "";
		
		System.out.println(x.charAt(0));
		System.out.println(x.charAt(1));
		System.out.println(x.charAt(2));
		
		char y = x.charAt(0);
		char y1 = x.charAt(1);
		char y2 = x.charAt(2);
		

	}

}
