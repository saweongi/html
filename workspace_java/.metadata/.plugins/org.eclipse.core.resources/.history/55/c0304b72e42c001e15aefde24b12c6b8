package ch09_class.future;

public class FutureStudent {
	
	//전역 변수를 사용하려면 클래스 수준에서 정적(static)필드를 선언해야함.
	
	private static int cnt =1;
	
	//학생 상태필드
	private int stuId;
	private String name;
	private String enName;
	private int level;
	private int exp;
	
	public FutureStudent(int stuId, String name, String enName, int level, int exp) {
	
		this.stuId = cnt;
		this.name = name;
		this.enName = enName;
		this.level = level;
		this.exp = exp;
	}
	
	
}
