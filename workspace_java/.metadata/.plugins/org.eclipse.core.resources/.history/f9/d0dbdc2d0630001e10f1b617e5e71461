package ch10_extends_interface.simple;

public class Parent {
	private String name;
	private int age;
	public Parent(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}public void sayHello() {
		System.out.println("부모입니다.");
	}
	@Override
	public String toString() {
		return "Parent [name=" + name + ", age=" + age + "]";
	}

	
}
