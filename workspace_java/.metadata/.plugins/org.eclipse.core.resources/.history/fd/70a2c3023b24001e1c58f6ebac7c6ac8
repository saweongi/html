package ch04_operator;

public class OperatorMain {

	public static void main(String[] args) {
	
	int num = 10;
	System.out.println(num);
	num ++;
	System.out.println(num);
	num --;
	System.out.println(num);
	++num;
	System.out.println(num);
	--num;
	System.out.println(num);
	
	int a, b;
	a = num ++;
	b = ++num;
	System.out.printf("a: %d 는 할당후 증가, b: %d 는 증가후 할당.", a,b);
	System.out.println(a);
		
	}

}
