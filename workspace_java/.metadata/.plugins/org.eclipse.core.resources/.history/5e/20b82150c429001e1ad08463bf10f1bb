package ch07_array;

public class ArrayMain {

	public static void main(String[] args) {
		String samjangString = "삼장";
		String woogong = "오공";
		String saojeong ="사오정";
		String palgye ="저팔계";
		
		String [] seougi = new String[4];
		System.out.println(seougi.length);
		System.out.println(seougi[0]);
	}

}
